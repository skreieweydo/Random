{
	"name": "@skreio/random",
	"version": "1.0.4",
	"description": "Tiny random utilities for JS/TS (predictable errors, no mutation).",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/skreieweydo/random.git"
	},
	"homepage": "https://github.com/skreieweydo/random#readme",
	"bugs": {
		"url": "https://github.com/skreieweydo/random/issues"
	},
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./utils": {
			"types": "./dist/utils/index.d.ts",
			"import": "./dist/utils/index.js"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE",
		"CHANGELOG.md"
	],
	"sideEffects": false,
	"engines": {
		"node": ">=18"
	},
	"directories": {
		"test": "tests"
	},
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"test": "jest",
		"test:coverage": "jest --coverage"
	},
	"keywords": [
		"random",
		"utilities",
		"javascript",
		"typescript",
		"bounded random",
		"math",
		"array",
		"prng"
	],
	"author": "skreieweydo",
	"devDependencies": {
		"@babel/core": "^7.23.5",
		"@babel/preset-env": "^7.23.5",
		"@babel/preset-typescript": "^7.23.3",
		"@types/jest": "^29.5.11",
		"@types/node": "^24.3.1",
		"babel-jest": "^29.7.0",
		"jest": "^29.7.0",
		"typescript": "^5.9.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
